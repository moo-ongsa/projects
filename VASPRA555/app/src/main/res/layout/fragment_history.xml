<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="362dp"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".HistoryFragment">


    <TextView
        android:id="@+id/textView21"
        android:layout_width="118dp"
        android:layout_height="59dp"
        android:layout_marginTop="36dp"
        android:text="ประวัติ"
        android:textColor="#BA7C7C"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_historybath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/loginbutton"
        android:text="ใส่บาตร"
        app:layout_constraintBottom_toTopOf="@+id/Layout_History"
        app:layout_constraintEnd_toStartOf="@+id/btn_historyproject"
        app:layout_constraintHorizontal_bias="0.693"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_historyproject"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_historyproject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/loginbutton"
        android:text="โครงการ"
        app:layout_constraintBottom_toTopOf="@+id/Layout_History"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btn_historysub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/loginbutton"
        android:text="รายเดือน"
        app:layout_constraintBottom_toTopOf="@+id/Layout_History"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.306"
        app:layout_constraintStart_toEndOf="@+id/btn_historyproject"
        app:layout_constraintTop_toBottomOf="@+id/textView21"
        app:layout_constraintVertical_bias="1.0" />

    <FrameLayout
        android:id="@+id/Layout_History"
        android:layout_width="match_parent"
        android:layout_height="486dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>